fn boolean_literals() {
    let t = true;
    let f = false;
}

fn string_literals() {
    let hello = "Hello, World!";
    let empty = "";
}

fn character_literals() {
    let a = 'a';
    let newline = '\n';
    let tab = '\t';
}

fn integer_literals() {
    let default_int = 42;
    let explicit_i32 = 123i32;
    let explicit_i64 = 456i64;
    let unsigned_32 = 789u32;
    let unsigned_64 = 101112u64;
}

fn float_literals() {
    let default_float = 3.14;
    let explicit_f32 = 2.5f32;
    let explicit_f64 = 1.618f64;
}